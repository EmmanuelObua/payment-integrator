{
	"name": "emmanuel-obua/payment-integrator",
	"description": "Integrate online payments from different service providors",
	"type": "library",
	"require": {
		"php": "^8.1",
		"illuminate/support": "^10.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.2",
		"scrutinizer/ocular": "^1.9",
		"satooshi/php-coveralls": "^0.6.1",
		"mockery/mockery": "^1.6"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "EmmanuelObua",
			"email": "eobua6882@gmail.com"
		}
	],
	"autoload": {
		"classmap": [
		    "src/Rave.php"
		],
		"psr-4": {
			"BlackJew\\Rave\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"cs-fix": "php-cs-fixer fix",
		"cs-lint": "php-cs-fixer fix --dry-run",
		"doc": "doctum.php update doctum.php -v",
		"test": "phpunit",
		"test-coverage": "phpunit --coverage-html coverage"
	},
	"extra": {
	    "laravel": {
	        "providers": [
	            "BlackJew\\Rave\\RaveServiceProvider"
	        ],
	        "aliases": {
	            "Rave": "BlackJew\\Rave\\Facades\\Rave"
	        }
	    }
	}
}
